                  "example": "Rodrigues"
                },
                "lastName": {
                  "example": "Cavalcante"
                },
                "email": {
                  "example": "melissa@gmail.com"
                },
                "password": {
                  "example": "Melissa123@"
                },
                "accessLevel": {
                  "example": 1
                }
              }
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          },
          "400": {
            "description": "Primeiro e último nome são obrigatórios / E-mail informado é inválido / A senha não coincide com os padrões requisitados"
